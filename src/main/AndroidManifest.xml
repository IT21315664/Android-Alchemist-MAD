<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" >

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.Career"
        tools:targetApi="31" >
        <provider
            android:name="androidx.core.content.FileProvider"
            android:authorities="com.yourapp.fileprovider"
            android:exported="false"
            android:grantUriPermissions="true">
            <meta-data
                android:name="android.support.FILE_PROVIDER_PATHS"
                android:resource="@xml/file_provider_paths"/>
        </provider>

        <activity
            android:name=".update_Dialog"
            android:exported="false" />
        <activity
            android:name=".activities.EmployeeDetails"
            android:exported="false" />
        <activity
            android:name=".emp_list_item"
            android:exported="false" />
        <activity
            android:name=".activities.FetchDetails"
            android:exported="false" />
        <activity
            android:name=".activities.insertandfetch"
            android:exported="false" />
        <activity
            android:name=".activities.form"
            android:exported="false" />
        <activity
            android:name=".activities.CareerDetails"
            android:exported="false" />
        <activity
            android:name=".activities.MainActivity"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>